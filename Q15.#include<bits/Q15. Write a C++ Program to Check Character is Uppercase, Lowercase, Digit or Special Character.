#include<bits/stdc++.h>
using namespace std;

int main(){
    char c;
    cin>>c;
    if(c>=97&&c<=122)
    cout<<"Lowercase\n";
    else if(c>=65&&c<=90)
    cout<<"Uppercase\n";
    else if(c>=48&&c<=57)
    cout<<"Digit\n";
    else if(c>=33&&c<=43)
    cout<<"Special Character\n";
    else
    cout<<"Invalid Input\n";
   
    return 0;
}
